<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pens.lab.app.belajaractivity">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:usesCleartextTraffic="true"
        android:name=".Starter"
        android:allowBackup="true"
        android:icon="@mipmap/icon_b"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_b"
        android:supportsRtl="true"
        android:theme="@style/BlueTheme">
        <activity android:name=".modul.input.InputActivity"></activity>
        <activity android:name=".modul.edit.EditActivity"></activity>
        <activity android:name=".modul.main.MainActivity"></activity>
        <activity android:name=".modul.register.RegisterActivity"></activity>
        <activity android:name=".modul.login.LoginActivity"></activity>
        <activity android:name=".modul.todo.ToDoActivity"></activity>
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".utils.AlarmReceiver"/>
    </application>

</manifest>